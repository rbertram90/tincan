    "behat/mink-browserkit-driver": "^2.1.0",
    "behat/mink-selenium2-driver": "^1.7.0",
    "friends-of-behat/mink-extension": "^2.7.5",
    "friendsofphp/php-cs-fixer": "^3.11",
    "phpunit/phpunit": "^9",
